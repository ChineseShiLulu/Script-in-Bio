    "# PCA降维\n",
    "pbmc <- RunPCA(pbmc, features = VariableFeatures(object = pbmc), verbose=FALSE)\n",
    "head(pbmc@meta.data)\n",
    "\n",
    "# 可视化\n",
    "VizDimLoadings(pbmc, dims = 1:2, reduction = \"pca\")\n",
    "DimPlot(pbmc, reduction = \"pca\")\n",
    "ElbowPlot(pbmc)"
   ]
