    "# 计算每个细胞中线粒体（MT）基因的百分比\n",
    "pbmc[[\"percent.mt\"]] <- PercentageFeatureSet(pbmc, pattern = \"^MT-\")\n",
    "# 计算每个细胞中核糖体（以 RPS 或 RPL 开头）基因的百分比\n",
    "pbmc[[\"percent.rb\"]] <- PercentageFeatureSet(pbmc, pattern = \"^RP[SL]\")\n",
    "head(pbmc@meta.data)\n",
    "\n",
