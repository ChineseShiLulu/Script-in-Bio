       "\tAAACCGTGCTTCCG-1 & pbmc3k & 2639 &  960 & 1.7430845 & 24.25161\\\\\n",
       "\tAAACCGTGTATGCG-1 & pbmc3k &  980 &  521 & 1.2244898 & 14.89796\\\\\n",
       "\tAAACGCACTGGTAC-1 & pbmc3k & 2163 &  781 & 1.6643551 & 36.19972\\\\\n",
